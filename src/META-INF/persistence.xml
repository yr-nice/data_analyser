    <class>com.mu.stock.entity.Company</class>
    <class>com.mu.stock.entity.DailyPriceLog</class>
    <class>com.mu.stock.entity.RealtimeDataLog</class>
    <class>com.mu.stock.entity.Account</class>
    <class>com.mu.stock.entity.AccountLog</class>
